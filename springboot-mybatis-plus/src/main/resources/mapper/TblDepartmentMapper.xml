<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gankki.mybatis.mapper.TblDepartmentMapper">

    <select id="selectById1" resultType="java.lang.String">
        select id from tbl_department
    </select>


    <insert id="insert1">
        insert into mc_drgs_warn_indicator (`customer_id`, `create_time`)
        values (
                   #{customerId},
                   #{createTime}
               )
    </insert>
</mapper>